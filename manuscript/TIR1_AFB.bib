  keywords = {Arabidopsis,Ubiquitin,Phytohormone,Saccharomyces},
  pages = {905-909},
  file = {/Users/clayw/Google Drive/References/PDF_library/Guseman et al_2015_Auxin-induced degradation dynamics set the pace for lateral root development.pdf;/Users/clayw/Google Drive/References/PDF_library/Guseman et al_2015_Auxin-induced degradation dynamics set the pace for lateral root development2.pdf;/Users/clayw/Google Drive/References/PDF_library/Guseman et al_2015_Auxin-induced degradation dynamics set the pace for lateral root development3.pdf;/Users/clayw/Zotero/storage/3IVW279P/905.html},
